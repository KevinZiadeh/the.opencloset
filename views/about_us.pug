extends layout

block content
  .about_intro Meet our team, the open closet family
  .container-fluid
    .row
      .col-md-4(style='text-align: -webkit-center')
        .card
          img(src='images/about/chady.jpg')
          h1.card_name Chady Najjar
          p.title CEO & Founder
          p.card_text Ecole Superieure des Affaires
          .contact_bar
            a.contact_icon(href='https://www.facebook.com/CHADYYYYYY')
