            a.contact_icon(href='https://www.instagram.com/chadi_najjar/')
              i.fab.fa-instagram
            a.contact_icon(href='')
              .fab.fa-linkedin
              
      .col-md-4(style='text-align: -webkit-center')
        .card
          img(src='images/about/kevin.jpeg')
          h1.card_name Kevin Ziadeh
